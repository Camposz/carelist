{
    "expo": {
        "name": "carelist",
        "slug": "carelist",
        "icon": "assets/images/store-assets/icon.png",
        "orientation": "portrait",
        "splash": {
            "image": "assets/images/store-assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#FAF6F4"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.camposz.carelist"
        },
        "android": {
            "package": "com.camposz.carelist",
            "adaptivelcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#FAF6F4"
            }
        },
        "plugins": [
            [
                "expo-build-properties",
                {
                    "ios": {
                        "newArchEnabled": true
                    },
                    "android": {
                        "newArchEnabled": true
                    }
                }
            ]
        ]
    }
}
